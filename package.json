{
  "name": "my-qwik-library-name",
  "version": "0.0.1",
  "description": "Create a Qwik library",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc -p tsconfig.types.json --emitDeclarationOnly --incremental false",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "start": "vite --open --mode ssr",
    "test": "echo \"No test specified\" && exit 0",
    "qwik": "qwik",
    "vitest": "vitest run attributes.spec.tsx"
  },
  "peerDependencies": {
    "@qwik.dev/core": "2.0.0-beta.13"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@qwik.dev/core": "2.0.0-beta.13",
    "@qwik.dev/router": "latest",
    "@types/node": "24.3.0",
    "@vitest/coverage-istanbul": "4.0.14",
    "eslint": "9.33.0",
    "eslint-plugin-qwik": "2.0.0-beta.13",
    "globals": "16.4.0",
    "np": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0",
    "vite": "7.1.11",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^4.0.14"
  }
}
